{
  "name": "defily",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cp .env.development .env && craco start",
    "build": "cp .env.production .env && craco build",
    "eject": "react-scripts eject",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format:write": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md,ts,tsx}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@craco/craco": "^6.1.2",
    "@headlessui/react": "^1.2.0",
    "@reduxjs/toolkit": "^1.5.1",
    "@uniswap/token-lists": "^1.0.0-beta.25",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.1",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.6",
    "ethers": "^5.4.1",
    "global": "^4.4.0",
    "kardia-js-sdk": "^0.4.5",
    "node-sass": "^6.0.1",
    "prop-types": "^15.7.2",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-countdown": "^2.3.2",
    "react-countup": "^4.3.3",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-ga": "^3.3.0",
    "react-hook-form": "^7.9.0",
    "react-multi-carousel": "^2.6.3",
    "react-paginate": "^7.1.3",
    "react-range": "^1.8.9",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-table": "^7.7.0",
    "react-toastify": "^7.0.4",
    "react-window": "^1.8.6",
    "recharts": "^2.0.10",
    "redux-localstorage-simple": "^2.4.1",
    "web3": "^1.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "autoprefixer": "^9.8.6",
    "eslint": "^7.26.0",
    "husky": "4.3.5",
    "lint-staged": "^11.0.0",
    "postcss": "^7.0.35",
    "prettier": "^2.3.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
